<!DOCTYPE html>
<html lang="zxx" class="no-js">

<head>
  <!-- Mobile Specific Meta -->
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
  <!-- Favicon-->
  <link rel="shortcut icon" href="img/fav.png" />
  <!-- Author Meta -->
  <meta name="author" content="CodePixar" />
  <!-- Meta Description -->
  <meta name="description" content="" />
  <!-- Meta Keyword -->
  <meta name="keywords" content="" />
  <!-- meta character set -->
  <meta charset="UTF-8" />
  <!-- Site Title -->
  <title>Audiogalore</title>

  <!--
		CSS
		============================================= -->
  <link rel="stylesheet" href="../fe/css/linearicons.css" />
  <link rel="stylesheet" href="../fe/css/owl.carousel.css" />
  <link rel="stylesheet" href="../fe/css/themify-icons.css" />
  <link rel="stylesheet" href="../fe/css/font-awesome.min.css" />
  <link rel="stylesheet" href="../fe/css/nice-select.css" />
  <link rel="stylesheet" href="../fe/css/nouislider.min.css" />
  <link rel="stylesheet" href="../fe/css/bootstrap.css" />
  <link rel="stylesheet" href="../fe/css/main.css" />
  <script src="https://unpkg.com/easyinvoice/dist/easyinvoice.min.js"></script>
</head>

<body>
  <!-- Start Header Area -->
  <%- include('../layouts/userHeader.ejs')%>
    <!-- End Header Area -->

    <!--================Order Details Area =================-->



    <section class="order_details section_gap">
      <div class="container">
        <h3 style="margin-top: 100px" class="title_confirmation">
          Thank you. Your order has been received.
        </h3>
        <div class="row order_d_inner" style="display: flex; justify-content: center">
          <div class="col-lg-4">
            <div class="details_item">
              <h4>Order Info</h4>
              <ul class="list">
                <li>
                  <a href="#"><span>Order Number</span>
                    <%=order.id%>
                  </a>
                </li>

                <li>
                  <a href="#"><span>Date</span>
                    <%=order.date%>
                  </a>
                </li>
                <li>
                  <a href="#"><span>Total</span>₹<%=order.total%></a>
                </li>
                <li>
                  <a href="#"><span>Payment method</span>
                    <%=order.payment%>
                  </a>
                </li>
              </ul>
            </div>
          </div>

          <div class="col-lg-4">
            <div class="details_item">
              <h4>Shipping Address</h4>
              <ul class="list">
                <li>
                  <a href="#"><span>Nmae</span>
                    <%=order.name%>
                  </a>
                </li>

                <li>
                  <a href="#"><span>Street</span>
                    <%=order.street%>
                  </a>
                </li>
                <li>
                  <a href="#"><span>Locality</span>
                    <%=order.locality%>
                  </a>
                </li>
                <li>
                  <a href="#"><span>City</span>
                    <%=order.city%>
                  </a>
                </li>
                <li>
                  <a href="#"><span>State </span>
                    <%=order.state%>
                  </a>
                </li>
                <li>
                  <a href="#"><span>Pincode </span>
                    <%=order.pincode%>
                  </a>
                </li>
              </ul>
            </div>
          </div>
        </div>
        <div class="order_details_table">
          <h2>Order Details</h2>
          <div class="table-responsive">
            <table class="table">
              <thead>
                <tr>
                  <th scope="col">Product</th>
                  <th style="padding-left: 0px" scope="col">Quantity</th>
                  <th scope="col">Total</th>
                </tr>
              </thead>
              <products>
                <tbody>
                  <% for (let i=0; i < order.product.length; i++) { %>
                    <tr>
                      <td>
                        <p>
                          <%=order.product[i].productName%>
                        </p>
                      </td>
                      <td>
                        <h5>
                          <%=order.product[i].quantity%>
                        </h5>
                      </td>
                      <td>
                        <p>
                          <%= order.product[i].productPrice * order.product[i].quantity %>
                        </p>
                      </td>
                    </tr>

                    <% } %>
                      <tr>
                        <td>
                          <h5>Sub Total</h5>
                        </td>
                        <td></td>
                        <td>₹<%=order.total%>
                        </td>
                      </tr>
                </tbody>
              </products>
            </table>
          </div>
        </div>
        <div class="row " style="margin-top: 30px;">
          <div class="col-lg-4">
            <a href="/download-invoice" class="download-invoice-btn">
              <i class="fa fa-download"></i> Download Invoice
            </a>

        
      </div>
     
    </section>


    <!--================End Order Details Area =================-->

    <!-- start footer Area -->

    <%- include('../layouts/userFooter.ejs')%>

      <!-- End footer Area -->

      <script src="../fe/js/vendor/jquery-2.2.4.min.js"></script>
      <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.../fe/js/1.11.0/umd/popper.min.js"
        integrity="sha384-b/U6ypiBEHpOf/4+1nzFpr53nxSS+GLCkfwBdFNTxtclqqenISfwAzpKaMNFNmj4"
        crossorigin="anonymous"></script>
      <script src="../fe/js/vendor/bootstrap.min.js"></script>
      <script src="../fe/js/jquery.ajaxchimp.min.js"></script>
      <script src="../fe/js/jquery.nice-select.min.js"></script>
      <script src="../fe/js/jquery.sticky.js"></script>
      <script src="../fe/js/nouislider.min.js"></script>
      <script src="../fe/js/jquery.magnific-popup.min.js"></script>
      <script src="../fe/js/owl.carousel.min.js"></script>
      <!--gmaps Js-->
      <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCjCGmQ0Uq4exrzdcL6rvxywDDOvfAu6eE"></script>
      <script src="../fe/js/gmaps.min.js"></script>
      <script src="../fe/js/main.js"></script>


      


<!--  -->










</body>

</html>